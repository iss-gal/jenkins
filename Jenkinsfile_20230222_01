def a = 10
def b = 20
pipeline
{
    agent any
    stages
    {
        stage ("Creacion de Archivo")
        {
            steps
            {
                script
                {
                    def c = (a*b)
                    println c
                    writeFile(file: "/var/lib/jenkins/workspace/Exercicio5/text.txt", text: c)
                    def lectura = readFile(file: "/var/lib/jenkins/workspace/Exercicio5/text.txt")
                    println lectura
                }
            }
        }
    }
}
